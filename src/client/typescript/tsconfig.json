{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "dist",
    "target": "es6",
    "moduleResolution": "node",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "lib": [
      "dom",
      "es6"
    ]
  },
    "include": [
      "src/**/*"
    ],
  "exclude": [
    "node_modules"
  ]
}
